{
  "name": "@bandofai/unido-dev",
  "version": "0.1.3",
  "description": "Unido Dev - Development server and tools for widget development",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bandofai/unido.git",
    "directory": "packages/dev"
  },
  "bugs": {
    "url": "https://github.com/bandofai/unido/issues"
  },
  "homepage": "https://github.com/bandofai/unido#readme",
  "author": "Tomas Kavka <iam@tomaskavka.cz>",
  "license": "MIT",
  "keywords": [
    "unido",
    "dev",
    "development",
    "widget",
    "server"
  ],
  "dependencies": {
    "@bandofai/unido-core": "^0.1.6",
    "@bandofai/unido-provider-base": "^0.1.7",
    "vite": "^6.0.5",
    "@vitejs/plugin-react": "^4.3.4",
    "chokidar": "^4.0.3"
  },
  "peerDependencies": {
    "@bandofai/unido-core": "^0.1.6",
    "@bandofai/unido-provider-base": "^0.1.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
