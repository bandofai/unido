{
  "name": "@bandofai/unido-provider-openai",
  "version": "0.1.27",
  "description": "Unido OpenAI - Provider adapter for OpenAI ChatGPT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapter.js": {
      "types": "./dist/adapter.d.ts",
      "import": "./dist/adapter.js"
    },
    "./schema.js": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./resource.js": {
      "types": "./dist/resource.d.ts",
      "import": "./dist/resource.js"
    },
    "./server.js": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bandofai/unido.git",
    "directory": "packages/providers/openai"
  },
  "bugs": {
    "url": "https://github.com/bandofai/unido/issues"
  },
  "homepage": "https://github.com/bandofai/unido#readme",
  "author": "Tomas Kavka <iam@tomaskavka.cz>",
  "license": "MIT",
  "keywords": [
    "unido",
    "openai",
    "chatgpt",
    "provider",
    "mcp",
    "ai"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "esbuild": "^0.25.0",
    "express": "^5.1.0",
    "postcss-import": "^16.1.1",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "peerDependencies": {
    "@bandofai/unido-core": "^0.1.3",
    "@bandofai/unido-provider-base": "^0.1.3"
  },
  "devDependencies": {
    "@bandofai/unido-core": "workspace:*",
    "@bandofai/unido-provider-base": "workspace:*",
    "@types/node": "^22.10.7",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^18.3.5",
    "react": "^19.2.0",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
