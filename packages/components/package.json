{
  "name": "@bandofai/unido-components",
  "version": "0.1.1",
  "description": "Unido Components - Universal UI components for AI applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo src/**/*.js src/**/*.js.map src/**/*.d.ts src/**/*.d.ts.map"
  },
  "dependencies": {
    "@bandofai/unido-core": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bandofai/unido.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/bandofai/unido/issues"
  },
  "homepage": "https://github.com/bandofai/unido#readme",
  "author": "Tomas Kavka <iam@tomaskavka.cz>",
  "license": "MIT",
  "keywords": [
    "unido",
    "components",
    "ui",
    "react",
    "ai"
  ]
}
